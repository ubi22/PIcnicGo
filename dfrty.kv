# Menu item in the DrawerList list.
#:import utils kivy.utils
<Contentd>
    orientation: "vertical"
    BoxLayout:
        orientation: "horizontal"

<sDrawer>:

    orientation: "horizontal"
    padding: "10dp"






<ContentNavigationDrawer>:
    orientation: "vertical"
    padding: "8dp"
    spacing: "8dp"

    AnchorLayout:
        anchor_x: "left"
        size_hint_y: None
        height: avatar.height

        Image:
            id: avatar
            size_hint: None, None
            size: "56dp", "56dp"
            source: "/PicnicGo_версия 1.3/Иконки/главный.jpg"
        MDNavigationDrawerHeader:
            title: "Header title"
            title_color: "#4a4939"
            text: "Header text"



    ScrollView:

        DrawerList:
            id: md_list
            OneLineAvatarListItem:
                on_release: app.on_fg()
                text: "О нас"
                IconLeftWidget:
                    icon: "information"
            OneLineAvatarListItem:
                on_release: app.show_date_picker()
                text: "Календарь"
                IconLeftWidget:
                    icon: "calendar"
            OneLineAvatarListItem:
                on_release: app.show_ate_picker()
                text: "Выход"
                IconLeftWidget:
                    icon: "exit-to-app"


Screen:
    screen_manager: screen_manager
    nav_drawer: nav_drawer

    MDNavigationLayout:

        ScreenManager:
            id: screen_manager
            MDScreen:
                screen_manager: screen_manager
                nav_drawer: nav_drawer

                MDBottomNavigation:
                    panel_color: 1, 1, 1, 0


                    MDBottomNavigationItem:
                        name: 'screen 1'
                        text: 'Калькулятор'
                        icon: 'calculator-variant'

                        BoxLayout:
                            orientation: 'vertical'

                            BoxLayout:
                                orientation: "vertical"
                                TextInput:
                                    id:calc_input
                                    font_size: dp(60)
                                    text: "0"
                                    halign: "left"
                                    size_hint: 1 , 0.2
                                GridLayout:
                                    spacing: 3
                                    padding: 3
                                    size_hint: 1 , 0.6
                                    cols:4
                                    MDRaisedButton:
                                        text: "AC"
                                        size_hint: 0.20 , 0.55
                                        font_size: dp(20)
                                        theme_text_color: "Custom"
                                        text_color: 1, 1, 1, 1
                                        md_bg_color: 0.6, 0.6, 0.6, 1
                                        on_press: app.clear()
                                    MDRaisedButton:
                                        id: clear
                                        text: "<--"
                                        size_hint: 0.20 , 0.55
                                        on_press: app.remove()
                                        font_size: dp(20)

                                        theme_text_color: "Custom"
                                        text_color: 1, 1, 1, 1
                                        md_bg_color: 0.6, 0.6, 0.6, 1
                                    MDRaisedButton:
                                        text: "/"
                                        size_hint: 0.20 , 0.55
                                        font_size: dp(20)
                                        theme_text_color: "Custom"
                                        text_color: 1, 1, 1, 1
                                        md_bg_color: 0.6, 0.6, 0.6, 1
                                        on_press: app.divide()
                                    MDRaisedButton:
                                        text: "x"
                                        size_hint: 0.20 , 0.55
                                        theme_text_color: "Custom"
                                        text_color: 1, 1, 1, 1
                                        font_size: dp(20)
                                        md_bg_color: 0.6, 0.6, 0.6, 1
                                        on_press: app.multiply()
                                    MDRaisedButton:
                                        text: "7"
                                        size_hint: 0.20 , 0.55
                                        theme_text_color: "Custom"
                                        font_size: dp(20)
                                        text_color: 0.01, 0.01, 0.01, 1
                                        md_bg_color: 0.85, 0.85, 0.85, 1
                                        on_press: app.button_press(7)
                                    MDRaisedButton:
                                        text: "8"
                                        size_hint: 0.20 , 0.55
                                        font_size: dp(20)
                                        theme_text_color: "Custom"
                                        text_color: 0.01, 0.01, 0.01, 1
                                        md_bg_color: 0.85, 0.85, 0.85, 1
                                        on_press: app.button_press(8)
                                    MDRaisedButton:
                                        text: "9"
                                        size_hint: 0.20 , 0.55
                                        font_size: dp(20)
                                        theme_text_color: "Custom"
                                        text_color: 0.01, 0.01, 0.01, 1
                                        md_bg_color: 0.85, 0.85, 0.85, 1
                                        on_press: app.button_press(9)
                                    MDRaisedButton:
                                        text: "-"
                                        size_hint: 0.20 , 0.55
                                        font_size: dp(20)
                                        theme_text_color: "Custom"
                                        text_color: 1, 1, 1, 1
                                        md_bg_color: 0.6, 0.6, 0.6, 1
                                        on_press: app.subtract()

                                    MDRaisedButton:
                                        text: "4"
                                        size_hint: 0.20 , 0.55
                                        font_size: dp(20)
                                        theme_text_color: "Custom"
                                        text_color: 0.01, 0.01, 0.01, 1
                                        md_bg_color: 0.85, 0.85, 0.85, 1
                                        on_press: app.button_press(4)
                                    MDRaisedButton:
                                        text: "5"
                                        size_hint: 0.20 , 0.55
                                        font_size: dp(20)
                                        theme_text_color: "Custom"
                                        text_color: 0.01, 0.01, 0.01, 1
                                        md_bg_color: 0.85, 0.85, 0.85, 1
                                        on_press: app.button_press(5)
                                    MDRaisedButton:
                                        text: "6"
                                        size_hint: 0.20 , 0.55
                                        font_size: dp(20)
                                        theme_text_color: "Custom"
                                        text_color: 0.01, 0.01, 0.01, 1
                                        md_bg_color: 0.85, 0.85, 0.85, 1
                                        on_press: app.button_press(6)
                                    MDRaisedButton:
                                        text: "+"
                                        size_hint: 0.20 , 0.55
                                        font_size: dp(20)
                                        theme_text_color: "Custom"
                                        text_color: 1, 1, 1, 1
                                        md_bg_color: 0.6, 0.6, 0.6, 1
                                        on_press: app.add()

                                    MDRaisedButton:
                                        text: "1"
                                        size_hint: 0.20 , 0.55
                                        font_size: dp(20)
                                        theme_text_color: "Custom"
                                        text_color: 0.01, 0.01, 0.01, 1
                                        md_bg_color: 0.85, 0.85, 0.85, 1
                                        on_press: app.button_press(1)
                                    MDRaisedButton:
                                        text: "2"
                                        size_hint: 0.20 , 0.55
                                        font_size: dp(20)
                                        theme_text_color: "Custom"
                                        text_color: 0.01, 0.01, 0.01, 1
                                        md_bg_color: 0.85, 0.85, 0.85, 1
                                        on_press: app.button_press(2)
                                    MDRaisedButton:
                                        text: "3"
                                        size_hint: 0.20 , 0.55
                                        font_size: dp(20)
                                        theme_text_color: "Custom"
                                        text_color: 0.01, 0.01, 0.01, 1
                                        md_bg_color: 0.85, 0.85, 0.85, 1
                                        on_press: app.button_press(3)
                                    MDRaisedButton:
                                        id: inp0
                                        text: "0"
                                        size_hint: 0.20 , 0.55
                                        font_size: dp(20)
                                        theme_text_color: "Custom"
                                        text_color: 1, 1, 1, 1
                                        md_bg_color: 0.6, 0.6, 0.6, 1
                                        on_press: app.button_press(0)
                                BoxLayout
                                    size_hint: 1 , 0.2
                                    orientation: "horizontal"
                                    MDRaisedButton:
                                        font_size: dp(20)
                                        text: "="
                                        size_hint: 1 , 1
                                        theme_text_color: "Custom"
                                        text_color: 1, 1, 1, 1
                                        md_bg_color: 0.6, 0.6, 0.6, 1
                                        on_press: app.equals()

                    MDBottomNavigationItem:
                        name: 'screen 2'
                        text: 'Главная'
                        icon: 'home'

                        BoxLayout:
                            orientation: 'vertical'

                            MDTopAppBar:
                                title: "Picnic"
                                elevation: 10
                                left_action_items: [['menu', lambda x: nav_drawer.set_state("open")]]
                                md_bg_color: 0, 0, 0, 1


                            BoxLayout:
                                orientation: "vertical"
                                spacing: "10dp"
                                padding: 0, "10dp", "10dp", "10dp"
                                ScrollView:
                                    scroll_timeout : 100
                                    MDList:
                                        id: md_list
                                        padding: 0
                                        BoxLayout:
                                            orientation: "horizontal"
                                            padding: "10dp"
                                            FitImage:
                                                source: "/PicnicGo_версия 1.3/Иконки/денги.png"
                                                size_hint: 0.25, .85
                                            MDTextField:
                                                id: bud
                                                hint_text: "Какой бюджет?"
                                                mode: "fill"
                                                fill_color_normal: 0, 0, 0, 1



                                        BoxLayout:
                                            orientation: "horizontal"
                                            padding: "10dp"
                                            MDTextField:
                                                id: coli
                                                hint_text: "Сколько людей?"
                                                mode: "fill"


                                        BoxLayout:
                                            orientation: "horizontal"
                                            padding: "10dp"
                                            MDTextField:
                                                hint_text: "Вы вегетарианец?"
                                                mode: "fill"
                                                md_bg_color: 0, 0, 0, 1
                                                max_text_length: 1
                                            MDRaisedButton:
                                                id: vega
                                                size_hint: .35, .70
                                                text: "Выбрать"
                                                theme_text_color: "Custom"
                                                text_color: 0, 0, 0, 1
                                                md_bg_color: 0.85, 0.85, 0.85, 1
                                                on_release: app.show_alert_dialog()



                                        BoxLayout:
                                            orientation: "horizontal"
                                            padding: "10dp"
                                            FitImage:
                                                source: "/PicnicGo_версия 1.3/Иконки/восход.jpg"
                                                size_hint: 0.25, .85
                                            MDTextField:
                                                id: ftt
                                                hint_text: "На сколька дней?"
                                                mode: "fill"
                                                md_bg_color: 0, 0, 0, 1
                                                max_text_length:



                                        BoxLayout:
                                            orientation: "horizontal"
                                            padding: "10dp"
                                            MDTextField:
                                                hint_text: "Куда поедите?"
                                                mode: "fill"
                                                max_text_length: 1
                                            MDRaisedButton:
                                                id: i
                                                size_hint: .35, .70
                                                text: "Выбрать"
                                                theme_text_color: "Custom"
                                                text_color: 0, 0, 0, 1
                                                md_bg_color: 0.85, 0.85, 0.85, 1
                                                on_release: app.show_aler_dialog()

                                        BoxLayout:
                                            orientation: "horizontal"
                                            padding: "10dp"
                                            MDTextField:
                                                hint_text: "В какой сезон?"
                                                mode: "fill"
                                                max_text_length: 1
                                            MDRaisedButton:
                                                id: u
                                                size_hint: .35, .70
                                                text: "Выбрать"
                                                theme_text_color: "Custom"
                                                text_color: 0, 0, 0, 1
                                                md_bg_color: 0.85, 0.85, 0.85, 1
                                                on_release: app.show_alerr_dialog()
                                        BoxLayout:
                                            orientation: "vertical"
                                            padding: "10dp"
                                            MDRaisedButton:
                                                text: "Вести"
                                                theme_text_color: "Custom"
                                                text_color: 0, 0, 0, 1
                                                md_bg_color: 0.85, 0.85, 0.85, 1

                                                on_release: app.vivod()
                                                size_hint: 1, .50


                    MDBottomNavigationItem:
                        name: 'screen 3'
                        text: 'Продукты'
                        source: "/PicniGo_1.4/Иконки/45200.png"

                        MDLabel

